{
  "name": "blogsley-django",
  "version": "1.0.0",
  "description": "Blogsley Django Demo Site",
  "main": "index.js",
  "scripts": {
    "newenv": "python3 -m venv env",
    "setup": "pip install -r requirements.txt",
    "build": "webpack",
    "freeze": "pip freeze | grep -v 'pkg-resources' > requirements.txt",
    "createsuper": "python manage.py createsuperuser",
    "makemigrations": "python manage.py makemigrations",
    "migrate": "python manage.py migrate",
    "dump": "python manage.py dumpdata --indent 2 > dump.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "git push dokku master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blogsley/blogsley-django.git"
  },
